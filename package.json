{
  "name": "boom-client",
  "version": "0.2.1",
  "private": true,
  "author": "@harkl_",
  "scripts": {
    "dev": "cp ./.env.dev ./.env && vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "deploy:ar": "arweave deploy-dir build --key-file ",
    "deploy:prod": "bash ./scripts/prod-deploy.sh",
    "format:fix": "prettier --write \"**/*.+(js|jsx|ts|tsx|json|css|md)\"",
    "gen:graphql": "graphql-codegen --config codegen.yml",
    "lint": "npx prettier --write ./src",
    "tslint": "tslint --fix src/**/*{.ts,.tsx}",
    "localnet:logs": "solana logs -u localhost",
    "localnet:reset": "solana-test-validator --reset",
    "localnet:up": "solana-test-validator --bpf-program BpfProgram1111111111111111111111111111111111 program/target/deploy/sosol_program.so",
    "predeploy": "git pull --ff-only && yarn && yarn build",
    "test": "node scripts/test.js",
    "test-with-coverage": "node scripts/test.js --coverage"
  },
  "dependencies": {
    "@apollo/client": "^3.6.8",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@identity.com/solana-gateway-ts": "^0.8.2",
    "@ledgerhq/hw-transport-webusb": "^6.0.2",
    "@liqnft/candy-shop": "^0.5.22",
    "@metaplex-foundation/mpl-token-metadata": "^1.2.5",
    "@metaplex/js": "^4.12.0",
    "@mui/icons-material": "^5.10.16",
    "@mui/lab": "^5.0.0-alpha.111",
    "@mui/material": "^5.10.17",
    "@project-serum/anchor": "^0.24.2",
    "@project-serum/serum": "^0.13.65",
    "@project-serum/sol-wallet-adapter": "^0.2.5",
    "@sentry/react": "^7.40.0",
    "@sentry/tracing": "^7.40.0",
    "@solana/spl-token": "^0.1.8",
    "@solana/spl-token-registry": "^0.2.168",
    "@solana/spl-token-swap": "0.1.0",
    "@solana/wallet-adapter-base": "^0.9.22",
    "@solana/wallet-adapter-material-ui": "^0.16.28",
    "@solana/wallet-adapter-react": "^0.15.30",
    "@solana/wallet-adapter-wallets": "^0.19.15",
    "@solana/web3.js": "^1.73.3",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "axios": "0.21.1",
    "bigint-buffer": "^1.1.5",
    "bignumber.js": "^9.0.1",
    "bn.js": "^5.2.0",
    "browserslist": "^4.18.1",
    "bs58": "^4.0.1",
    "buffer-layout": "^1.2.2",
    "camelcase": "^6.2.1",
    "crypto-browserify": "^3.12.0",
    "cuid": "^2.1.8",
    "dayjs": "^1.11.5",
    "dotenv": "^10.0.0",
    "emoji-mart": "^3.0.1",
    "eventemitter3": "^4.0.7",
    "graphql": "^15.5.1",
    "jazzicon": "^1.5.0",
    "linkify-plugin-hashtag": "^3.0.3",
    "linkify-plugin-mention": "^3.0.3",
    "linkify-react": "^3.0.3",
    "linkifyjs": "^3.0.5",
    "lodash": "^4.17.21",
    "moment": "2.29.1",
    "react": "^18.1.0",
    "react-apollo": "3.1.5",
    "react-countdown": "^2.3.2",
    "react-dom": "^18.1.0",
    "react-helmet": "^6.1.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-onesignal": "^2.0.4",
    "react-parallax-tilt": "^1.6.1",
    "react-router-dom": "^6.4.2",
    "react-router-hash-link": "^2.4.3",
    "reactjs-popup": "^2.0.5",
    "recoil": "^0.7.3-alpha.1",
    "simple-react-lightbox": "^3.6.9-0",
    "string-to-color": "^2.2.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.2.2",
    "@graphql-codegen/typescript": "2.3.0",
    "@graphql-codegen/typescript-operations": "^2.2.0",
    "@graphql-codegen/typescript-react-apollo": "3.2.0",
    "@originjs/vite-plugin-commonjs": "^1.0.3",
    "@tsconfig/recommended": "^1.0.1",
    "@types/bn.js": "^5.1.0",
    "@types/bs58": "^4.0.1",
    "@types/emoji-mart": "^3.0.9",
    "@types/giphy-api": "^2.0.3",
    "@types/jest": "^26.0.23",
    "@types/ledgerhq__hw-transport": "^4.21.3",
    "@types/ledgerhq__hw-transport-webusb": "^4.70.1",
    "@types/lodash": "^4.14.177",
    "@types/node": "^16.0.0",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.3",
    "@types/react-helmet": "^6.1.5",
    "@types/react-router-dom": "^5.3.2",
    "@types/react-router-hash-link": "^2.4.5",
    "@types/react-test-renderer": "^17.0.1",
    "@types/simple-react-lightbox": "^3.6.1",
    "@vitejs/plugin-react": "^3.1.0",
    "arweave-deploy": "^1.9.1",
    "babel-preset-env": "^1.7.0",
    "prettier": "^2.3.2",
    "react-test-renderer": "^17.0.2",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vite-plugin-node-polyfills": "^0.7.0",
    "vite-plugin-svgr": "^2.4.0",
    "vite-tsconfig-paths": "^4.0.5"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/boom-army/boom-client"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/config/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|cjs|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "resetMocks": true
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
